{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "signal",
  "compatibility_date": "2025-09-06",
  "assets": {
    "directory": "./public"
  },
  "observability": {
    "enabled": true
  },
  "main": "worker.js",
  "durable_objects": {
    "bindings": [
      {
        "name": "WEBSOCKET_SERVER",
        "class_name": "WebSocketServer"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "WebSocketServer"
      ]
    }
  ]
}
